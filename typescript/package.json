{
  "name": "@coral-xyz/xnft",
  "version": "0.2.40",
  "license": "GPL-3.0-only",
  "description": "Node.js client for the xNFT protocol",
  "repository": {
    "type": "git",
    "url": "https://github.com/coral-xyz/xnft.git"
  },
  "homepage": "https://github.com/coral-xyz/xnft/blob/master/typescript#readme",
  "bugs": {
    "url": "https://github.com/coral-xyz/xnft/issues"
  },
  "keywords": [
    "solana",
    "xnft",
    "protocol",
    "coral",
    "web3"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=11"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "browser": "./dist/browser/index.js",
  "scripts": {
    "build": "yarn clean && yarn build:node && yarn build:browser",
    "build:browser": "rollup --config",
    "build:node": "tsc && tsc -p ./tsconfig.cjs.json",
    "clean": "rm -rf ./dist",
    "lint": "eslint .",
    "test": "jest",
    "prepublishOnly": "yarn lint && yarn test",
    "prepack": "yarn build && cp ../LICENSE . && cp ../README.md .",
    "postpack": "rm LICENSE README.md"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.26.0",
    "@metaplex-foundation/js": "^0.17.12",
    "@solana/spl-token": "^0.3.7",
    "@solana/web3.js": "^1.73.0",
    "semver": "^7.3.8",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "rollup": "^3.10.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
