  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.17.6",
    "@project-serum/anchor": "^0.25.0",
    "@solana/spl-token": "^0.3.6",
    "@solana/web3.js": "^1.66.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
